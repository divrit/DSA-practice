class Solution {
    public String reverseWords(String s) {
        


        s = s.trim();
        String xx[] = s.split(" ");
        StringBuilder sb = new StringBuilder();
        for(int i=xx.length-1; i>= 0; i--){

            if (!xx[i].isEmpty()) {
                sb.append(xx[i]);
                if (i > 0) {
                    sb.append(" ");
                }
            }

}
        return sb.toString();
    }
}
