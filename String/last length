class Solution {
    public int lengthOfLastWord(String s) {
        int count = 0;
       

        String ss  = s.trim();
    
        for(int i = ss.length()-1 ; i >= 0; i--){
                               count++;

                if(ss.charAt(i) == ' '){
                    count--;
                    break;
                }


        }


        return  count;
    }
}
